<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="mil.navy.spawar.adapt.jaguarcontroller.JaguarMenu" >

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:onClick="onClickController"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="@string/controller"
        android:textSize="14sp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:onClick="onClickMapView"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="@string/mapView"
        android:textSize="14sp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:onClick="onClickLIDARData"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="@string/lidarData"
        android:textSize="14sp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:onClick="onClickMessageTraffic"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="@string/messageTraffic"
        android:textSize="14sp" />


